
 <%-include("partials/header.ejs")-%>
<div class= "container">
    <h1></h1>
    <form>
        <div class="form-group row">
          <label for="name" class="col-sm-3 col-form-label">Name</label>
          <div class="col-sm-8">
            <input type="name" class="form-control" id="name"aria-describedby="name">
          </div>
        </div>
        <div class="form-group row">
            <label for="agency" class="col-sm-3 col-form-label">Agency</label>
            <div class="col-sm-8">
            <input type="agency" class="form-control" id="agency"aria-describedby="agency">
            </div>
        </div>
        <div class="form-group row">
            <label for="DID" class="col-sm-3 col-form-label">DID</label>
            <div class="col-sm-8">
             <input type="DID" class="form-control" id="DID"aria-describedby="DID">
            </div>
        </div>
        <div class="form-group row">
            <label for="HP" class="col-sm-3 col-form-label">HP</label>
            <div class="col-sm-8">
            <input type="HP" class="form-control" id="HP"aria-describedby="HP">
            </div>
        </div>
        <div class="form-group row">
            <label for="email" class="col-sm-3 col-form-label">Email</label>
            <div class="col-sm-8">
            <input type="email" class="form-control" id="email"aria-describedby="email">
            </div>
        </div>
        <div class="form-group row">
            <label for="address" class="col-sm-3 col-form-label">Address</label>
            <div class="col-sm-8">
            <input type="address" class="form-control" id="address"aria-describedby="address">
            </div>
        </div>
       <div class="row"> <button type="button" class="btn btn-primary">Create Contact</button></div>
      
       <div id="qr2"></div>
       <div id="qr_vcard"></div>

       <%var testContact = contact.Name %>
       <div id="qr1"><%qrCode.createVCard(testContact)%></div>
       <% document.getElementById('vcard').innerHTML = qrCode.createVCard(testContact)%>

    </form>
</div>
 
